<body>
    <form action="usuario" method="post">
        <label for="DNI">DNI:</label>
        <input type="text"  name="formDni" id="dni" placeholder="11.111.111-A">
        <br>
        <label for="Nombre">Nombre:</label>
        <input type="text" name="formUsuarioNombre" id="nombre" placeholder="Nombre">
        <br>
        <label for="Apellidos">Apellidos:</label>
        <input type="text" name="formApellidos" id="apellidos" placeholder="Apellidos">
        <br>
        <label for="contraseña">Contraseña:</label>
        <input type="text" name="formContrasena" id="apellidos" placeholder="Entre 8-10 caracteres">
        <br>
        <label for="Email">Email:</label>
        <input type="text" name="formEmail" placeholder="Email">
        <br>
        <label for="Telefono">Telefono:</label>
        <input type="text" name="formTelefono" id="telefono" placeholder="+34 999999999">
        <br>
        <button>Enviar</button>
    </form>             
            
</body>

</html>